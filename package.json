    "react-native-keep-awake": "^4.0.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-maps": "^0.27.0",
    "react-native-modal": "^12.0.0",
    "react-native-push-notification": "^4.0.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "^2.0.0",
    "react-native-sensors": "5.3.3",
    "react-native-side-menu-updated": "^1.3.2",
    "react-native-slowlog": "^1.0.2",
    "react-native-sqlite-2": "1.7.0",
    "react-native-svg": "^9.3.5",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^4.1.0",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.1.0",
    "react-timer-mixin": "^0.13.3",
    "redux": "4.0.3",
    "redux-actions": "2.2.1",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.2.0",
    "redux-thunk-error-handler": "^1.1.4",
    "regression": "^2.0.1",
    "relational-pouch": "^3.2.0",
    "styled-components": "^5.1.0",
    "typescript": "^4.6.3"
