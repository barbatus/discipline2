    "check-types": "5.0.0",
    "color": "^3.1.2",
    "eventemitter3": "4.0.0",
    "events": "^3.0.0",
    "haversine": "^1.0.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "2.20.1",
    "moment-duration-format": "^2.1.1",
    "pouchdb-adapter-react-native-sqlite": "^2.0.0",
    "pouchdb-find": "7.0.0",
    "pouchdb-react-native": "^6.4.1",
    "promise": "7.0.4",
    "prop-types": "15.6.0",
    "react": "17.0.2",
    "react-mixin": "^4.0.0",
    "react-native": "0.65.2",
