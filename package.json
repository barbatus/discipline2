{
  "name": "app",
  "version": "0.7.0",
  "author": "Alex <alex.borodach@gmail.com>",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test2": "jest --silent=false",
    "bundle": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output iOS/main.jsbundle",
    "lint:staged": "lint-staged",
    "prettify": "prettier --single-quote=true --trailing-comma=all --write **/**/*.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint -- . ",
    "react-devtools": "react-devtools",
    "build:ios": "react-native run-ios",
    "upgrade": "react-native-git-upgrade",
    "clear": "watchman watch-del ./ && npm start -- --reset-cache",
    "link": "react-native link"
  },
  "dependencies": {
    "assert": "1.3.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "check-types": "5.0.0",
    "eventemitter3": "1.1.1",
    "haversine": "^1.0.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "2.20.1",
    "moment-duration-format": "^2.1.1",
    "pouchdb-adapter-react-native-sqlite": "^1.0.3",
    "pouchdb-find": "6.4.0",
    "pouchdb-react-native": "6.3.4",
    "promise": "7.0.4",
    "prop-types": "15.6.0",
    "react": "^16.3.0-alpha.1",
    "react-mixin": "^2.0.2",
    "react-native": "0.54.0",
    "react-native-background-geolocation": "^2.10.1",
    "react-native-device-info": "^0.13.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-maps": "^0.19.0",
    "react-native-navbar": "^2.1.0",
    "react-native-shake-event": "^4.0.0",
    "react-native-side-menu": "^1.1.3",
    "react-native-slowlog": "^1.0.2",
    "react-native-sqlite-2": "^1.5.2",
    "react-navigation": "^1.5.11",
    "react-redux": "^5.0.6",
    "react-timer-mixin": "^0.13.3",
    "recompose": "^0.26.0",
    "redux": "3.5.2",
    "redux-actions": "2.2.1",
    "redux-form": "^7.0.3",
    "redux-thunk": "^2.1.0",
    "relational-pouch": "^3.0.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "jest": "19.0.0",
    "lint-staged": "^3.3.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "react-devtools": "^3.2.1",
    "react-test-renderer": "~16.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "airbnb-flow",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "flowtype"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "import/resolver": "reactnative"
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/sort-comp": [
        2,
        {
          "order": [
            "static-methods",
            "type-annotations",
            "instance-variables",
            "lifecycle",
            "/^on.+$/",
            "/^(get|set)(?!(InitialState$|DefaultProps$|ChildContext$)).+$/",
            "everything-else",
            "/^render.+$/",
            "render"
          ]
        }
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0,
      "import/no-webpack-loader-syntax": 0,
      "allowSamePrecedence": 0,
      "no-mixed-operators": [
        2,
        {
          "allowSamePrecedence": true
        }
      ]
    }
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./__tests__/setup.js"
    ],
    "testRegex": "(/__tests__/.*-test)\\.jsx?$"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote=true --trailing-comma=all --write",
      "git add"
    ]
  }
}
