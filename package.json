{
  "name": "app",
  "version": "0.7.0",
  "author": "Alex <alex.borodach@gmail.com>",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test2": "jest --silent=false",
    "bundle": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output iOS/main.jsbundle",
    "lint:staged": "lint-staged",
    "prettify": "prettier --single-quote=true --trailing-comma=all --write **/**/*.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern __tests__",
    "lint:js": "npm run lint:eslint -- .",
    "lint:js:fix": "npm run lint:eslint -- . --fix",
    "react-devtools": "react-devtools",
    "build:ios": "react-native run-ios",
    "upgrade": "react-native upgrade",
    "clear": "watchman watch-del ./ && npm start -- --reset-cache",
    "link": "react-native link",
    "flow": "flow",
    "certs:ios": "fastlane match development && fastlane match appstore",
    "fastlane:ios": "fastlane ios beta"
  },
  "dependencies": {
    "@okgrow/react-native-copilot": "^2.3.0",
    "assert": "1.3.0",
    "check-types": "5.0.0",
    "eventemitter3": "1.1.1",
    "haversine": "^1.0.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "2.20.1",
    "moment-duration-format": "^2.1.1",
    "pouchdb-adapter-react-native-sqlite": "^1.0.3",
    "pouchdb-find": "6.4.2",
    "pouchdb-react-native": "6.3.4",
    "promise": "7.0.4",
    "prop-types": "15.6.0",
    "react": "16.8.3",
    "react-mixin": "^2.0.2",
    "react-native": "0.59.1",
    "react-native-background-geolocation": "^2.12.2",
    "react-native-background-timer": "^2.1.1",
    "react-native-device-info": "^0.13.0",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-maps": "^0.23.0",
    "react-native-navbar": "^2.1.0",
    "react-native-shake": "^3.2.2",
    "react-native-side-menu": "^1.1.3",
    "react-native-slowlog": "^1.0.2",
    "react-native-sqlite-2": "1.7.0",
    "react-native-svg": "^9.3.5",
    "react-navigation": "^3.5.1",
    "react-redux": "^6.0.1",
    "react-timer-mixin": "^0.13.3",
    "recompose": "^0.30.0",
    "redux": "4.0.1",
    "redux-actions": "2.2.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.2.0",
    "relational-pouch": "^3.0.0",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.12.1",
    "flow-bin": "^0.92.0",
    "fsevents": "^1.2.7",
    "jest": "^24.5.0",
    "lint-staged": "^3.3.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "react-devtools": "^3.2.1",
    "react-test-renderer": "16.8.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "flowtype"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "import/resolver": "reactnative",
      "import/core-modules": [
        "Dimensions",
        "NativeMethodsMixin",
        "Easing"
      ]
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "operator-linebreak": [
        "error",
        "after"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_value"
          ]
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "consistent": true
        }
      ],
      "function-paren-newline": 0,
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/sort-comp": [
        2,
        {
          "order": [
            "static-methods",
            "type-annotations",
            "instance-variables",
            "lifecycle",
            "/^(get|set)(?!(InitialState$|DefaultProps$|ChildContext$)).+$/",
            "/^on(?!(.+Cb$)).+$/",
            "everything-else",
            "/^render(?!(ed$)).+$/",
            "render"
          ]
        }
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "react/destructuring-assignment": 0,
      "require-yield": 0,
      "import/no-webpack-loader-syntax": 0,
      "allowSamePrecedence": 0,
      "no-mixed-operators": [
        2,
        {
          "allowSamePrecedence": true
        }
      ]
    }
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./__tests__/setup.js"
    ],
    "testRegex": "(/__tests__/.*-test)\\.jsx?$"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote=true --trailing-comma=all --write",
      "git add"
    ]
  }
}
